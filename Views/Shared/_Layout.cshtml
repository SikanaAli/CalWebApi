<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CalWebApi</title>
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />
    <link rel="stylesheet" href="~/lib/izimodal/css/iziModal.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
    @*<link rel="stylesheet" href="~/lib/DataTables/DataTables-1.10.24/css/dtMaterialize.css" />*@
    <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/datatables.css" />
    
    <link rel="stylesheet" href="~/css/QuartzDashboard.css" />
    @RenderSection("Styles", required: false)
</head>
<body>
  
            <div class="col s12 cyan darken-1 z-depth-1 center" style="height:3rem">
                <ul class="tabs tabs-transparent right hide-on-med-and-down" id="myTab" role="tablist">
                    <li class="tab">
                        <a class="active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Home</a>
                    </li>
                    <li class="tab">
                        <a class="" id="calendar-tab" data-toggle="tab" href="#calendar" role="tab" aria-controls="calendar" aria-selected="false">Calendar</a>
                    </li>
                    <li class="tab">
                        <a class="" id="docs-tab" data-toggle="tab" href="#docs" role="tab" aria-controls="contact" aria-selected="false">Docs</a>
                    </li>
                </ul>
            </div>
        
    
    <div class="container" style="margin-top:2rem">
        <main role="main" class="row">

            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CalWebApi - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js" type="text/javascript"></script>
    <script src="~/lib/izimodal/js/iziModal.min.js"></script>
    <script src="~/lib/materialize/js/materialize.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    <script src="~/lib/DataTables/datatables.js" asp-append-version="true"></script>
    <script src="~/lib/DataTables/Buttons-1.7.0/js/buttons.bootstrap4.min.js" asp-append-version="true"></script>
    <script type="module" src="~/js/SchedularDashBoard.js" asp-append-version="true"></script>
</body>
</html>
