<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CalWebApi</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />
    <link rel="stylesheet" href="~/lib/izimodal/css/iziModal.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/DataTables/datatables.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Buttons-1.7.0/css/buttons.bootstrap4.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-sweetalert/sweetalert.min.css" />
    <link rel="stylesheet" href="~/css/QuartzDashboard.css" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <header>
        <div class="vw-100 top-banner border-bottom mb-1 bg-dark text-light">
            <strong id="next-refresh-time" class="text-center align-self-center"></strong>
        </div>
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">

            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CalWebApi - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js" type="text/javascript"></script>
    <script src="~/lib/izimodal/js/iziModal.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    <script src="~/lib/DataTables/datatables.js" asp-append-version="true"></script>
    <script src="~/lib/DataTables/Buttons-1.7.0/js/buttons.bootstrap4.min.js" asp-append-version="true"></script>
    <script type="module" src="~/js/SchedularDashBoard.js" asp-append-version="true"></script>
</body>
</html>
